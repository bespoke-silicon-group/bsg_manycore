.DEFAULT_GOAL = all

BENCHMARK_DIR = $(BSG_MANYCORE_DIR)/imports/greenlight/benchmarks/spec2000/CINT2000/256.bzip2

PROG_NAME = bzip2

MAX_CYCLES = 1000000000

BENCHMARK_OBJS = raw.o \
bzip2.o spec.o

INPUT_TYPE = lgred

BENCHMARK_INPS = $(INPUT_TYPE).source

$(BENCHMARK_INPS): %:
	cp -r $(BENCHMARK_DIR)/input/$@ $@

BENCHMARK_DEFS = -D__raw__

ARGC=3
ARGV=bzip2 $(BENCHMARK_INPS) 1

# lfs disk image size
LFS_BLOCK_SIZE = 128
LFS_BLOCK_COUNT = 16384 # 2 MB

CLEAN_ITEMS = $(BENCHMARK_INPS)

include ./Makefile.common
